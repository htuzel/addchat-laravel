{
    "name": "htuzel/addchat-laravel",
    "version": "1.0.2",
    "description": "Live chat widget for Turacoon.",
    "keywords": [
        "laravel",
        "laravel-package",
        "laravel chat package",
        "vuejs"
    ],
    "license": "MIT",
    "require": {
        "illuminate/support": "~5.5|~6.0"
    },
    "require-dev": {
        "laravel/framework": "~5.5|~6.0"
    },
    "autoload": {
        "psr-4": {
            "Classiebit\\Addchat\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable" : true,
    "extra": {
        "laravel": {
            "providers": [
                "Classiebit\\Addchat\\AddchatServiceProvider"
            ]
        }
    }
}
